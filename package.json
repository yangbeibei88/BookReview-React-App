{
  "name": "book-review-react-app",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run server2",
    "client": "vite --config client/vite.config.js",
    "server": "node server/server.js",
    "server2": "node server/server2.js",
    "dev": "concurrently \"npm run server2\" \"npm run client\"",
    "build": "npm run build-client",
    "build-client": "vite build --config client/vite.config.js",
    "format": "prettier --write \"client/src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"client/src/**/*.{js,jsx,ts,tsx}\" --quiet",
    "heroku-postbuild": "npm run build-client"
  },
  "keywords": [],
  "author": "Beibei Yang",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@vitejs/plugin-react": "^4.3.1",
    "cors": "^2.8.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "gh-pages": "^6.1.1",
    "prettier": "^3.3.2",
    "vite": "^5.3.3"
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "framer-motion": "^11.2.14",
    "json-server": "^0.17.4",
    "mongoose": "^8.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.24.1",
    "uuid": "^10.0.0"
  }
}
